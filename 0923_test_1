func = lambda x:x%2
result = filter(func, [1,2,3,4,5])
print(list(result))

str = 'hello,Python'
suffix = 'Python'
print(str.endswith(suffix , 2))
#true输出结果，endswith用于判断是否以suffix结尾，从str的第二位‘l’开始

sets = {1,2,3,4,5}
print(sets[2])
#输出报错，字典不支持索引
